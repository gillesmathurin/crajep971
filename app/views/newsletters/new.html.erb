<h1>New newsletter</h1>

<% form_for(@newsletter) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :published_at %><br />
    <%= f.datetime_select :published_at %>
  </p>
  <p>
    <%= f.label :titre %><br />
    <%= f.text_field :titre %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', newsletters_path %>