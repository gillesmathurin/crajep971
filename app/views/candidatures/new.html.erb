<h1>New candidature</h1>

<% form_for(@candidature) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nom_asso %><br />
    <%= f.text_field :nom_asso %>
  </p>
  <p>
    <%= f.label :adresse %><br />
    <%= f.text_field :adresse %>
  </p>
  <p>
    <%= f.label :cp %><br />
    <%= f.text_field :cp %>
  </p>
  <p>
    <%= f.label :ville %><br />
    <%= f.text_field :ville %>
  </p>
  <p>
    <%= f.label :tel %><br />
    <%= f.text_field :tel %>
  </p>
  <p>
    <%= f.label :fax %><br />
    <%= f.text_field :fax %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :nom_contact %><br />
    <%= f.text_field :nom_contact %>
  </p>
  <p>
    <%= f.label :tel_contact %><br />
    <%= f.text_field :tel_contact %>
  </p>
  <p>
    <%= f.label :objet %><br />
    <%= f.text_area :objet %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', candidatures_path %>