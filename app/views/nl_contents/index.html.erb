<h1>Listing nl_contents</h1>

<table>
  <tr>
    <th>Image file name</th>
    <th>Image content type</th>
    <th>Image file size</th>
    <th>Image updated at</th>
  </tr>

<% @nl_contents.each do |nl_content| %>
  <tr>
    <td><%=h nl_content.image_file_name %></td>
    <td><%=h nl_content.image_content_type %></td>
    <td><%=h nl_content.image_file_size %></td>
    <td><%=h nl_content.image_updated_at %></td>
    <td><%= link_to 'Show', nl_content %></td>
    <td><%= link_to 'Edit', edit_nl_content_path(nl_content) %></td>
    <td><%= link_to 'Destroy', nl_content, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New nl_content', new_nl_content_path %>